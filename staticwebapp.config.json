{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/icons/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/favicon.ico",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/icons/*",
      "/favicon.ico",
      "*.js",
      "*.css",
      "*.png",
      "*.jpg",
      "*.gif",
      "*.ico"
    ]
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
