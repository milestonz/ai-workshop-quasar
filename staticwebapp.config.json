{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "trailingSlash": "auto",
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=86400"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".md": "text/markdown"
  }
}
