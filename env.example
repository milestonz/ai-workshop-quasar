# Azure Blob Storage 설정
VITE_AZURE_STORAGE_ACCOUNT_NAME=your_storage_account_name
VITE_AZURE_STORAGE_ACCOUNT_KEY=your_storage_account_key
VITE_AZURE_STORAGE_CONTAINER_NAME=course-data

# 개발 환경에서는 LocalStorage 사용 (Azure 설정이 없을 경우)
# 프로덕션 환경에서는 Azure Blob Storage 사용

# 학생 모드 활성화 (true/false)
# true: 학생용 페이지 접근 가능
# false: 학생용 페이지 접근 불가 (관리자만 접근)
VITE_ENABLE_STUDENT_MODE=false

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id 